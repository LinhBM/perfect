<% provide :title, t(".title") %>
<div class="row">
  <div class="col-md-12">
    <div class="card padding-md margin-top-md">
      <table class="table table-hover">
        <thead>
          <tr>
            <th>Avatar</th>
            <th>Username</th>
            <th>Email</th>
            <th>Rating</th>
            <th>Role</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%= render @users %>
        </tbody>
      </table>
      <%= will_paginate %>
    </div>
  </div>
</div>

