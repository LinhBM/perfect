<h2><%= t ".title" %></h2>

<%= form_for resource, as: resource_name, url: password_path(resource_name),
  html: {method: :post} do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.email_field :email, placeholder: t("femail"), autofocus: true %><br/>
    <%= f.submit t(".submit") %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
