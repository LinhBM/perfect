<h2><%= t ".title" %></h2>
<div class="container">
  <%= form_for resource, as: resource_name,
    url: session_path(resource_name) do |f| %>
    <div class="field">
      <%= f.email_field :email, placeholder: t("femail"), autofocus: true %>
    </div>

    <div class="field">
      <%= f.password_field :password, placeholder: t("fpass"),
        autocomplete: "off" %>
    </div>

    <% if devise_mapping.rememberable? -%>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label t ".label15" %>
      </div>
    <% end -%>

    <div class="actions">
      <%= f.submit t(".title") %>
    </div>
  <% end %>

  <%= render "devise/shared/links" %>
</div>
